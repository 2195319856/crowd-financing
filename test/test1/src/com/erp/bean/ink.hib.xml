<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.erp.bean.Ink" table="ink_info">
        <id name="id" column="id">
        <!-- 主键生成策略  -->
        <generator class="native"></generator>
        </id>
        <!-- 2、用户字段 -->
        <property name="Internal_PN" ></property>
        <property name="Layer_Name" ></property>
        <property name="Min_Brige_Width" ></property>
        <property name="SM_Colour" ></property>
        <property name="Input_User" ></property>
        <property name="Input_User_CN" ></property>
        <property name="Date_Time" ></property>
        <property name="Status" ></property>
   
   
    </class>
</hibernate-mapping>