<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.erp.bean.Surface" table="surface_info">
		<id name="id" column="id">
			<!-- 主键生成策略 -->
			<generator class="native"></generator>
		</id>
		<!-- 2、用户字段 -->
		<property name="Internal_PN"></property>
		<property name="Layer_Name"></property>
		<property name="Soft_Gold_Area"></property>
		<property name="Hold_Gold_Area"></property>
		<property name="ENIG_Area"></property>
		<property name="ENEPIG_Area"></property>
		<property name="Tin_Area"></property>
		<property name="Surface_Mode"></property>
		<property name="OSP"></property>
		
		
		<property name="Input_User"></property>
		<property name="Input_User_CN"></property>
		<property name="Date_Time"></property>
		<property name="Status"></property>
	</class>
</hibernate-mapping>