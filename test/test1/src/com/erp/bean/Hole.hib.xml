<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.erp.bean.Hole" table="hole_info">
        <id name="id" column="id">
        <!-- 主键生成策略  -->
        <generator class="native"></generator>
        </id>
        <!-- 2、用户字段 -->
       <property name="Internal_PN" ></property>
        <property name="Layer_Name" ></property>
        <property name="Hole_Size" ></property>
        <property name="Hole_Count" ></property>
        <property name="Hole_Type" ></property>
        <property name="Via_Type" ></property>
        <property name="Cross_ADH" ></property>
        <property name="Hole_Depth" ></property>
        <property name="Aspect_Ratio" ></property>
        <property name="Input_User" ></property>
        <property name="Input_User_CN" ></property>
        <property name="Date_Time" ></property>
        <property name="Status" ></property>
    </class>
</hibernate-mapping>