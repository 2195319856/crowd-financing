<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.erp.bean.Die" table="die_info">
		<id name="id" column="id">
			<!-- 主键生成策略 -->
			<generator class="native"></generator>
		</id>
		<!-- 2、用户字段 -->
		<property name="Internal_PN"></property>
		<property name="Layer_Name"></property>
		<property name="Tooling_PN"></property>
		<property name="Tool_Type"></property>
		<property name="Material_Width"></property>
		<property name="Material_Length"></property>
		<property name="PIN_Count"></property>
		<property name="PIN_Size"></property>
		<property name="Punch_Cycle"></property>
		<property name="Scale_X"></property>
		<property name="Scale_Y"></property>
		<property name="Line_Totle"></property>
		<property name="Hole_Totle"></property>
		<property name="Material_Thick"></property>
		<property name="Material"></property>
		<property name="Mirror"></property>
		
		<property name="Input_User"></property>
		<property name="Input_User_CN"></property>
		<property name="Date_Time"></property>
		<property name="Status"></property>
	</class>
</hibernate-mapping>