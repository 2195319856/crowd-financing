<struts>
	<!-- 配置spring与Struts2的结合 -->
	<constant name="struts.objectFactory" value="spring"></constant>
	<!-- 动态方法调用 -->
	<constant name="struts.enable.DynamicMethodInvocation"
		value="true"></constant>

	<package name="p1" extends="struts-default">
		<action name="user" class="com.erp.action.UserAction">
			<result name="se">/index.jsp</result>
			<result name="login">/login.jsp</result>
			<result name="relogin">/test.jsp</result>
			<allowed-methods>login,updatePwd</allowed-methods>
		</action>
	</package>


	<package name="hole" extends="struts-default">
		<action name="hole" class="com.erp.action.HoleAction">
			<result name="list">/Hole_list.jsp</result>
			<result name="update" type="redirect">
				hole!HoleList.action
			</result>
			<result name="find">/update_Hole.jsp</result>
			<result name="findByLike">/Hole_list.jsp</result>
			<result name="findByLike1">/Hole_list1.jsp</result>
			<result name="findByLike2">/Hole_list1.jsp</result>
			<allowed-methods>HoleList,find,updateHole,holeFindByLike,holeFindByLike1,holeFindByLike2</allowed-methods>
		</action>
	</package>

